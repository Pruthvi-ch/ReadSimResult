Executable =  rungen.sh 
Universe   = vanilla
should_transfer_files = YES
when_to_transfer_output = ON_EXIT
Transfer_Input_Files = SingleMuPt100_hgcal_cfi.py, rungen.sh
x509userproxy = $ENV(X509_USER_PROXY)
use_x509userproxy = true
+BenchmarkJob = True
+MaxRuntime = 36000
Output = log/log_$(cluster)_$(process).stdout
Error  = log/log_$(cluster)_$(process).stderr
Log    = log/log_$(cluster)_$(process).condor

X=$(step)
Arguments  = Extended2026D86 $INT(X) 
Queue 10

